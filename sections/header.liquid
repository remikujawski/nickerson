          Products
        </button>
        <button class="aspect-square cursor-pointer h-full bg-white text-nicknavyblue-100 font-bold text-xl hover:bg-nicknavyblue-60 hover:text-white transition duration-200">
          <svg
            xmlns="http://www.w3.org/2000/svg"
            fill="none"
            viewBox="0 0 24 24"
            stroke-width="1.5"
            stroke="currentColor"
            class="size-10 align-middle mx-auto"
          >
            <path stroke-linecap="round" stroke-linejoin="round" d="M3.75 6.75h16.5M3.75 12h16.5m-16.5 5.25h16.5" />
          </svg>
        </button>
        {% comment %}
          {% for link in linklists['main-menu'].links %}
            {% if link.links != blank %}
              <div class="relative">
                <button type="button" class="btn inline-flex items-center text-base text-gray-500 hover:text-gray-900">
                  {{ link.title }}
                  {% render 'icon-dropdown' %}
                </button>

                  <div class="absolute z-10 -ml-5 mt-5 transform px-2 w-screen max-w-md sm:px-0 lg:ml-0 lg:left-1/2 lg:-translate-x-1/2">
                    <div class="shadow-lg border ring-black ring-opacity-5 overflow-hidden rounded-lg">
                      <div class="relative grid gap-6 bg-white px-5 py-6 sm:gap-8 sm:p-8">
                        <!-- CREATE THE SUBLINK -->
                        <a class="-m-3 p-3 flex items start rounded-lg hover:bg-gray-100">
                          <div class="ml-4">
                            <p class="text-gray-900 text-base font-medium">Sublink 1</p>
                          </div>
                        </a>

                        <a class="-m-3 p-3 flex items start rounded-lg hover:bg-gray-100">
                          <div class="ml-4">
                            <p class="text-gray-900 text-base font-medium">Sublink 2</p>
                          </div>
                        </a>

                        <a class="-m-3 p-3 flex items start rounded-lg hover:bg-gray-100">
                          <div class="ml-4">
                            <p class="text-gray-900 text-base font-medium">Sublink 3</p>
                          </div>
                        </a>
                      </div>
                    </div>
                  </div>

              </div>
            {% else %}
              <div class="relative">
                <a href="{{ link.url }}" class="btn text-base text-gray-500 hover:text-gray-900">{{ link.title }}</a>
              </div>
            {% endif %}
          {% endfor %}
        {% endcomment %}
